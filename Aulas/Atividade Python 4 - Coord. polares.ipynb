    "import math as mt\n",
    "def distance(r,teta,z):\n",
    "    ang = teta*mt.pi/180\n",
    "    raio = r*mt.cos(ang)\n",
    "    dist = mt.sqrt(raio**2+z**2)\n",
    "    return round(dist, 3)\n",
    "distance(2,90,2)"
   ]
